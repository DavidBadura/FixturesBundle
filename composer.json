{
    "name": "davidbadura/fixtures-bundle",
    "description": "A Symfony2 bundle to load yaml fixtures. Easy extendable, resolve object dependency and validate objects.",
    "keywords": ["symfony2", "fixtures", "faker", "doctrine", "yaml", "database", "orm"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "David Badura",
        "email" : "d.badura@gmx.de"
    }],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony" : ">=2.0",
        "davidbadura/fixtures": "dev-master"
    },
    "suggest": {
        "davidbadura/faker-bundle" : "1.0.*"
    },
    "require-dev": {
        "davidbadura/faker-bundle" : "1.0.*"
    },
    "autoload": {
        "psr-0": {
            "DavidBadura\\FixturesBundle\\": ""
        }
    },
    "target-dir" : "DavidBadura/FixturesBundle"
}
