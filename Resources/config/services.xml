<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="davidbadura_fixtures.fixture_loader.class">DavidBadura\FixturesBundle\FixtureLoader</parameter>
        <parameter key="davidbadura_fixtures.relation_manager.class">DavidBadura\FixturesBundle\RelationManager\RelationManager</parameter>
        <parameter key="davidbadura_fixtures.fixture_file_loader.class">DavidBadura\FixturesBundle\FixtureFileLoader</parameter>
        <parameter key="davidbadura_fixtures.fixture_type_loader.class">DavidBadura\FixturesBundle\FixtureTypeLoader</parameter>
    </parameters>

    <services>
        <service id="davidbadura_fixtures.fixture_loader" class="%davidbadura_fixtures.fixture_loader.class%">
            <argument type="service" id="davidbadura_fixtures.relation_manager" />
        </service>

        <service id="davidbadura_fixtures.relation_manager" class="%davidbadura_fixtures.relation_manager.class%" />

        <service id="davidbadura_fixtures.fixture_type_loader" class="%davidbadura_fixtures.fixture_type_loader.class%" />

        <service id="davidbadura_fixtures.fixture_file_loader" class="%davidbadura_fixtures.fixture_file_loader.class%" />
    </services>

</container>